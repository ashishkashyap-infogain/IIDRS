    <style>
        body {
            zoom: 90%;
            padding: 0px;
            background: url(../../images/ws_Horizon_2560x1600.jpg) no-repeat 0px 0px;
            min-height: 100vh;
            background-size: cover;
            background-attachment: fixed;
        }

        .rate {
            margin-left: 10px;
        }
    </style>
</head>
<body>
    <nav class="navbar navbar-dark sticky-top bg-dark flex-md-nowrap p-0">
        <a class="navbar-brand col-sm-3 col-md-2 mr-0" href="#" style="padding:1px; text-align:center; background-color:aliceblue; border:1px solid">
            <img src="~/images/infogain-logo.png" alt="Infogain Logo" style="width:75%; height:65px;">
        </a>
        @{
            if (Session["Admin"] != null)
            {
                <span style="color:white; font-weight:500; font-size:larger;margin-left:-250px;">Integrated Development Reporting System</span>
                <div class="btn-group mr-2" style="width:200px">
                    <a class="btn btn-primary btn-sm btn-outline-light" href="#">ETL</a>
                    <a class="btn btn-primary btn-sm btn-outline-light" href="#">EIM</a>
                </div>
                @*<input class="form-control form-control-dark w-50" type="text" placeholder="Search" aria-label="Search">*@
            }
            else if (Session["CustId"] != null)
            {
                <span style="color:white; font-weight:500; font-size:20px; margin-left:-700px;">Integrated Development Reporting System</span>
            }
        }
        <ul class="navbar-nav px-3">
            <li class="nav-item text-nowrap">
                <span style="color:white; font-weight:500; font-size:larger;padding:20px;">Hi, @Session["LogedUser"]</span>
                <button class="btn btn-danger">
                    <a class="nav-link" href="/Home/Logout" style="color:white">
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-log-out"><path d="M9 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h4"></path><polyline points="16 17 21 12 16 7"></polyline><line x1="21" y1="12" x2="9" y2="12"></line></svg>Sign out
                    </a>
                </button>
            </li>
        </ul>
    </nav>

    <div class="container-fluid">
        <div class="row">
            <nav class="col-md-2 d-none d-md-block bg-light sidebar">
                <div class="sidebar-sticky">
                    <ul class="nav flex-column" style="margin-top:30px">
                        @{
                            if (Session["Admin"] != null)
                            {
                                if (ViewBag.Title == "Dashboard - Admin")
