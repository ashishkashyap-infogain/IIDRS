                    </a>
                </li>
            }*@


                        @*else if (Session["CustId"] != null)
                        {
                        <li class="nav-item">
                            <a class="nav-link active" href="#">
                <span data-feather="bar-chart-2"></span>
                Survey<span class="sr-only">(current)</span>
            </a>
                        </li>
                        }
                        }*@

                    </ul>
                </div>
            </nav>

            <main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4">
                <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3 border-bottom">
                    @{if (Session["CustId"] != null)
                        {

                            <h1 class="h2">Survey</h1>
                        }
                        else if (Session["Admin"] != null)
                        {

                            if (ViewBag.Title == "Dashboard - Admin")
                            {
                                <h1 class="h2">Dashboard</h1>
                            }
                            else if (ViewBag.Title == "Admin - Survey")
                            {
                                <h1 class="h2">Survey Details</h1>
                            }
                            else if (ViewBag.Title == "Authorization")
                            {
                                <h1 class="h2">Authorization</h1>
                            }
                            else if (ViewBag.Title == "Contact Details")
                            {
                                <h1 class="h2">Contact</h1>

                            }
                            else if (ViewBag.Title == "Customer Management")
                            {
                                <h1 class="h2">Survey Summary</h1>
                            }
                            else if (ViewBag.Title == "Business Unit")
                            {
                                <h1 class="h2">Business Unit</h1>
                            }
                        }
                    }
                    <div class="btn-toolbar mb-2 mb-md-0">
                        <div class="btn-group mr-2">
                            @if (Session["CustId"] != null)
                            {
                                if (Session["DS"] != null)
                                {
                                    <a class="btn btn-secondary btn-sm btn-outline-light active" href="/Survey/Index">Delivery Management</a>
                                    <a class="btn btn-secondary btn-sm btn-outline-light" href="/Survey/ExIndex">Executive Management</a>
                                }
                                else if (Session["ES"] != null)
                                {
                                    <a class="btn btn-secondary btn-sm btn-outline-light" href="/Survey/Index">Delivery Management</a>
                                    <a class="btn btn-secondary btn-sm btn-outline-light active" href="/Survey/ExIndex">Executive Management</a>
                                }
                            }
                        </div>
                    </div>
                </div>
                <div class="container body-content">
                    @RenderBody()
                    <hr />
                    <footer>
                        <p>Copyright &copy; @DateTime.Now.Year Infogain. All rights reserved</p>
                    </footer>
                </div>
            </main>
        </div>
    </div>

