                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-activity"><polyline points="22 12 18 12 15 21 9 3 6 12 2 12"></polyline></svg>
                                        AI Analytics
                                    </a>
                                </li>
                                if (ViewBag.Title == "Authorization")
                                {
                                    <li class="nav-item">
                                        <a class="nav-link active" href="/Authorization/Index2">
                                            <span data-feather="users"></span>
                                            Authorization
                                        </a>
                                    </li>
                                }
                                else
                                {
                                    <li class="nav-item">
                                        <a class="nav-link " href="/Authorization/Index2">
                                            <span data-feather="users"></span>
                                            Authorization
                                        </a>
                                    </li>
                                }
                                if (ViewBag.Title == "Admin - Survey")
                                {
                                    <li class="nav-item">
                                        <a class="nav-link active" href="/Survey/SurveyManage">
                                            <span data-feather="bar-chart-2"></span>
                                            Survey
                                        </a>
                                    </li>
                                }
                                else
                                {
                                    <li class="nav-item">
                                        <a class="nav-link" href="/Survey/SurveyManage">
                                            <span data-feather="bar-chart-2"></span>
                                            Survey
                                        </a>
                                    </li>
                                }
                                if (ViewBag.Title == "Customer Management")
                                {
                                    <li class="nav-item">
                                        <a class="nav-link active" href="/Customer/OrgEdit">
                                            <span data-feather="users"></span>
                                            Survey Summary
                                        </a>
                                    </li>
                                }
                                else
                                {
                                    <li class="nav-item">
                                        <a class="nav-link" href="/Customer/OrgEdit">
                                            <span data-feather="users"></span>
                                            Survey Summary
                                        </a>
                                    </li>
                                }
                            }
                            if (ViewBag.Title == "Contact Details")
                            {
                                <li class="nav-item">
                                    <a class="nav-link active" href="/Contact/Index">
                                        <span data-feather="phone"></span>
                                        Contacts
                                    </a>
                                </li>
                            }
                            else
                            {
                                <li class="nav-item">
                                    <a class="nav-link" href="/Contact/Index">
                                        <span data-feather="phone"></span>
                                        Contacts
                                    </a>
                                </li>
                            }
                        if (ViewBag.Title == "Business Unit")
                                {
                                    <li class="nav-item">
                                        <a class="nav-link" href="/BusinessUnit/GetAllBU">
                                            <span data-feather="clipboard"></span>
                                            Business Unit
                                        </a>
                                    </li>
                                }
                                else
                                {
                                    <li class="nav-item">
                                        <a class="nav-link" href="/BusinessUnit/GetAllBU">
                                            <span data-feather="clipboard"></span>
                                            Business Unit
                                        </a>
                                    </li>
                                }
                        }
                        @*else if (ViewBag.Title == "Index")
        {
            <li class="nav-item">
                <a class="nav-link " href="/Admin/AdminActivities">
                    <span data-feather="home"></span>
                    Dashboard
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="#">

                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-activity"><polyline points="22 12 18 12 15 21 9 3 6 12 2 12"></polyline></svg>
                        AI Analytics
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link active" href="/Customer/Index2">
                        <span data-feather="users"></span>
                        Authorization
